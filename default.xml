<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <default remote="gumstix" revision="default"/>

  <!-- You can have as many remote repositories as you like -->

  <remote name="yocto" fetch="git://git.yoctoproject.org" />
  <remote name="oe" fetch="git://git.openembedded.org" />
  <remote name="gumstix" fetch="https://github.com/gumstix" />
  <remote name="qt5" fetch="https://github.com/meta-qt5" />
  <remote name="qt5-mirror" fetch="https://code.qt.io/cgit/yocto" />
  <remote name="qt5-extra" fetch="https://github.com/schnitzeltony" />
  <remote name="tegra" fetch="https://github.com/madisongh" />
  <remote name="ossytems" fetch="https://github.com/OSSystems" />
  <remote name="clang" fetch="https://github.com/kraj" />
  <remote name="ros" fetch="https://github.com/ros" />

  <!-- List all the repositories you want to fetch -->
  <!-- "name": git repository's name on hosting server
       "remote": the remote host from above (if not the default)
       "revision": commitish to checkout (if not the default)
       "path": directory where git repository gets cloned/synced -->

  <project name="poky" revision="refs/heads/dunfell" remote="yocto" />
  <project name="meta-ti" revision="refs/heads/dunfell"  remote="yocto" path="poky/meta-ti" />
  <project name="meta-raspberrypi" revision="refs/heads/dunfell"  remote="yocto" path="poky/meta-raspberrypi" />
  <project name="meta-openembedded" revision="refs/heads/dunfell"  remote="oe" path="poky/meta-openembedded" />
  <project name="meta-gumstix" revision="refs/heads/dunfell"  remote="gumstix" path="poky/meta-gumstix" />
  <project name="meta-gumstix-extras" revision="refs/heads/dunfell" remote="gumstix" path="poky/meta-gumstix-extras" />
  <!-- Official meta-qt5 repository is on qt5.14, lxqt requires qt5.15. This can be reverted once metq-qt5/metq-qt5 catches up to qt5.15 -->
  <!-- <project name="meta-qt5" revision="refs/heads/dunfell"  remote="qt5" path="poky/meta-qt5" /> -->
  <project name="meta-qt5.git" revision="refs/heads/5.15"  remote="qt5-mirror" path="poky/meta-qt5" />
  <project name="meta-tegra" revision="refs/heads/dunfell-l4t-r32.4.3" remote="tegra" path="poky/meta-tegra" />
  <project name="meta-java" revision="refs/heads/dunfell"  remote="yocto" path="poky/meta-java" />
  <project name="meta-browser" revision="refs/heads/master"  remote="ossytems" path="poky/meta-browser" />
  <project name="meta-clang" revision="refs/heads/dunfell"  remote="clang" path="poky/meta-clang" />
  <project name="meta-python2" revision="refs/heads/dunfell"  remote="oe" path="poky/meta-python2" />
  <project name="meta-arm" revision="refs/heads/dunfell"  remote="yocto" path="poky/meta-arm" />
  <project name="meta-qt5-extra" revision="refs/heads/master"  remote="qt5-extra" path="poky/meta-qt5-extra" />
  <project name="meta-ros" revision="5f8eaceece856f892602753d79e9f19920c4d077" remote="ros" path="poky/meta-ros" />

</manifest>

